{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.18.1",
      "env": {
        "EXPO_USE_FAST_RESOLVER": "1",
        "ORG_GRADLE_PROJECT_kotlinJvmTarget": "17"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug -PreactNativeArchitectures=arm64-v8a",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.18.1",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "node": "20.18.1",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
